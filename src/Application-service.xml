<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">
    <import resource="Application-public.xml"></import>
    <!--泛型不能实例化-->
    <bean id="baseServiceImp" class="com.ztc.shop.service.impl.BaseServiceImp" lazy-init="true">
        <property name="sessionFactory" ref="sessionFactory"></property>
    </bean>

    <bean id="CategoryServiceImp" class="com.ztc.shop.service.impl.CategoryServiceImp" parent="baseServiceImp"/>
    <bean id="AccountServiceImp" class="com.ztc.shop.service.impl.AccountServiceImp" parent="baseServiceImp"/>
</beans>